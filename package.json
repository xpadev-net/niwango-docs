{
  "name": "niwango-docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "npx tsc --noEmit --jsx react-jsx",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{tsx,ts,js,json,css,scss}\"",
    "lint": "npm run eslint&&npm run check-types",
    "lint:fix": "npm run format&&npm run eslint:fix&&npm run check-types",
    "prepare": "husky install"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.1",
    "@xpadev-net/niwango": "0.0.1-canary.20230906-4",
    "@xpadev-net/niwango-core": "0.0.2-canary.20230906-4",
    "jotai": "^2.3.1",
    "monaco-editor": "^0.41.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "sass": "^1.66.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/youtube": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@xpadev-net/material-icons": "^0.0.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "2.8.8",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
